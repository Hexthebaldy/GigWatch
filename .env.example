# OpenAI-compatible (Kimi K2)
OPENAI_API_KEY=
OPENAI_BASE_URL=https://api.moonshot.cn/v1
OPENAI_MODEL=kimi-k2-turbo-preview

# App
APP_TIMEZONE=Asia/Shanghai
DB_PATH=./data/gigwatch.sqlite
APP_PORT=3000

# Telegram 通知（可选，参考 docs/telegram-integration.md）
TELEGRAM_BOT_TOKEN=
TELEGRAM_CHAT_ID=

# Feishu Bot（可选，参考 docs/feishu-bot-setup.md）
# 飞书应用凭据：用于换取 tenant_access_token 并发送消息
FEISHU_APP_ID=
FEISHU_APP_SECRET=
# 事件订阅校验 token：需与飞书开放平台配置保持一致
FEISHU_VERIFICATION_TOKEN=
# 本地 webhook 监听端口与路径（默认 3001 + /feishu/events）
FEISHU_WEBHOOK_PORT=3001
FEISHU_WEBHOOK_PATH=/feishu/events
# 飞书开放平台 API 基础地址（一般不用改）
FEISHU_BASE_URL=https://open.feishu.cn
